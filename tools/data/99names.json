{
    "code": 200,
    "status": "success",
    "data": {
        "names": [
            {
                "number": 1,
                "name": "\u0627\u0644\u0631\u064e\u0651\u062d\u0652\u0645\u064e\u0646\u064f",
                "transliteration": "Ar Rahmaan",
                "translation": "The Most Merciful",
                "meaning": "The One who has plenty of mercy for the believers and the blasphemers in this world and especially for the believers in the hereafter.",
                "audio": "/audio/asma-ul-husna/rahman.mp3"
            },
            {
                "number": 2,
                "name": "\u0627\u0644\u0631\u064e\u0651\u062d\u0650\u064a\u0645\u064f",
                "transliteration": "Ar Raheem",
                "translation": "The Most Compassionate",
                "meaning": "The One who has plenty of mercy for the believers.",
                "audio": "/audio/asma-ul-husna/rahim.mp3"
            },
            {
                "number": 3,
                "name": "\u0627\u0644\u0652\u0645\u064e\u0644\u0650\u0643\u064f",
                "transliteration": "Al Malik",
                "translation": "The King",
                "meaning": "The Sovereign Lord, The One who has absolute ownership and authority over all creation.",
                "audio": "/audio/asma-ul-husna/malik.mp3"
            },
            {
                "number": 4,
                "name": "\u0627\u0644\u0652\u0642\u064f\u062f\u064f\u0651\u0648\u0633\u064f",
                "transliteration": "Al Quddus",
                "translation": "The Most Holy",
                "meaning": "The One who is pure from any imperfection and clear from children and adversaries.",
                "audio": "/audio/asma-ul-husna/quddus.mp3"
            },
            {
                "number": 5,
                "name": "\u0627\u0644\u0633\u064e\u0651\u0644\u0627\u064e\u0645\u064f",
                "transliteration": "As Salaam",
                "translation": "The Source of Peace",
                "meaning": "The One who grants peace and calm to all creation.",
                "audio": "/audio/asma-ul-husna/salam.mp3"
            },
            {
                "number": 6,
                "name": "\u0627\u0644\u0652\u0645\u064f\u0624\u0652\u0645\u0650\u0646\u064f",
                "transliteration": "Al Mu'min",
                "translation": "The Guardian of Faith",
                "meaning": "The One who grants safety and security to His creation.",
                "audio": "/audio/asma-ul-husna/mumin.mp3"
            },
            {
                "number": 7,
                "name": "\u0627\u0644\u0652\u0645\u064f\u0647\u064e\u064a\u0652\u0645\u0650\u0646\u064f",
                "transliteration": "Al Muhaymin",
                "translation": "The Guardian",
                "meaning": "The One who witnesses the saying and deeds of His creatures.",
                "audio": "/audio/asma-ul-husna/muhaimin.mp3"
            },
            {
                "number": 8,
                "name": "\u0627\u0644\u0652\u0639\u064e\u0632\u0650\u064a\u0632\u064f",
                "transliteration": "Al Azeez",
                "translation": "The Almighty",
                "meaning": "The strong, the defeater who is not defeated.",
                "audio": "/audio/asma-ul-husna/aziz.mp3"
            },
            {
                "number": 9,
                "name": "\u0627\u0644\u0652\u062c\u064e\u0628\u064e\u0651\u0627\u0631\u064f",
                "transliteration": "Al Jabbaar",
                "translation": "The Compeller",
                "meaning": "The One that nothing happens in His dominion except that which He willed.",
                "audio": "/audio/asma-ul-husna/jabbar.mp3"
            },
            {
                "number": 10,
                "name": "\u0627\u0644\u0652\u0645\u064f\u062a\u064e\u0643\u064e\u0628\u0650\u0651\u0631\u064f",
                "transliteration": "Al Mutakabbir",
                "translation": "The Supreme",
                "meaning": "The Supreme, The One who is supremely great and above all creation.",
                "audio": "/audio/asma-ul-husna/mutakabbir.mp3"
            },
            {
                "number": 11,
                "name": "\u0627\u0644\u0652\u062e\u064e\u0627\u0644\u0650\u0642\u064f",
                "transliteration": "Al Khaaliq",
                "translation": "The Creator",
                "meaning": "The One who brings everything from non-existence to existence.",
                "audio": "/audio/asma-ul-husna/khaliq.mp3"
            },
            {
                "number": 12,
                "name": "\u0627\u0644\u0652\u0628\u064e\u0627\u0631\u0650\u0626\u064f",
                "transliteration": "Al Baari",
                "translation": "The Maker of Order",
                "meaning": "The Maker, The Creator who has the power to materialize and proportion His creation.",
                "audio": "/audio/asma-ul-husna/bari.mp3"
            },
            {
                "number": 13,
                "name": "\u0627\u0644\u0652\u0645\u064f\u0635\u064e\u0648\u0650\u0651\u0631\u064f",
                "transliteration": "Al Musawwir",
                "translation": "The Shaper of Beauty",
                "meaning": "The Fashioner, The One who forms His creatures in different shapes and forms.",
                "audio": "/audio/asma-ul-husna/musawwir.mp3"
            },
            {
                "number": 14,
                "name": "\u0627\u0644\u0652\u063a\u064e\u0641\u064e\u0651\u0627\u0631\u064f",
                "transliteration": "Al Ghaffaar",
                "translation": "The Forgiving",
                "meaning": "The One who forgives the sins of His slaves time and time again.",
                "audio": "/audio/asma-ul-husna/ghaffar.mp3"
            },
            {
                "number": 15,
                "name": "\u0627\u0644\u0652\u0642\u064e\u0647\u064e\u0651\u0627\u0631\u064f",
                "transliteration": "Al Qahhaar",
                "translation": "The Subduer",
                "meaning": "The Subduer, The One who has perfect Power and absolute control over all things.",
                "audio": "/audio/asma-ul-husna/qahhar.mp3"
            },
            {
                "number": 16,
                "name": "\u0627\u0644\u0652\u0648\u064e\u0647\u064e\u0651\u0627\u0628\u064f",
                "transliteration": "Al Wahhaab",
                "translation": "The Bestower",
                "meaning": "The One who is Generous in giving plenty without any return.",
                "audio": "/audio/asma-ul-husna/wahhab.mp3"
            },
            {
                "number": 17,
                "name": "\u0627\u0644\u0631\u064e\u0651\u0632\u064e\u0651\u0627\u0642\u064f",
                "transliteration": "Ar Razzaaq",
                "translation": "The Provider",
                "meaning": "The One who provides all the creatures their needs.",
                "audio": "/audio/asma-ul-husna/razzaq.mp3"
            },
            {
                "number": 18,
                "name": "\u0627\u0644\u0652\u0641\u064e\u062a\u064e\u0651\u0627\u062d\u064f",
                "transliteration": "Al Fattaah",
                "translation": "The Opener",
                "meaning": "The One who opens for His slaves the closed worldly and religious matters.",
                "audio": "/audio/asma-ul-husna/fattah.mp3"
            },
            {
                "number": 19,
                "name": "\u0627\u064e\u0644\u0652\u0639\u064e\u0644\u0650\u064a\u0652\u0645\u064f",
                "transliteration": "Al 'Aleem",
                "translation": "The All-Knowing",
                "meaning": "The One nothing is absent from His knowledge.",
                "audio": "/audio/asma-ul-husna/alim.mp3"
            },
            {
                "number": 20,
                "name": "\u0627\u0644\u0652\u0642\u064e\u0627\u0628\u0650\u0636\u064f",
                "transliteration": "Al Qaabid",
                "translation": "The Constrictor",
                "meaning": "The One who constricts the sustenance by His wisdom.",
                "audio": "/audio/asma-ul-husna/qabid.mp3"
            },
            {
                "number": 21,
                "name": "\u0627\u0644\u0652\u0628\u064e\u0627\u0633\u0650\u0637\u064f",
                "transliteration": "Al Baasit",
                "translation": "The Expander",
                "meaning": "The One who expands and widens.",
                "audio": "/audio/asma-ul-husna/basit.mp3"
            },
            {
                "number": 22,
                "name": "\u0627\u0644\u0652\u062e\u064e\u0627\u0641\u0650\u0636\u064f",
                "transliteration": "Al Khaafid",
                "translation": "The Abaser",
                "meaning": "The One who lowers whoever He willed by His Destruction.",
                "audio": "/audio/asma-ul-husna/khafid.mp3"
            },
            {
                "number": 23,
                "name": "\u0627\u0644\u0631\u064e\u0651\u0627\u0641\u0650\u0639\u064f",
                "transliteration": "Ar Raafi'",
                "translation": "The Exalter",
                "meaning": "The One who raises whoever He willed by His endowment.",
                "audio": "/audio/asma-ul-husna/rafi.mp3"
            },
            {
                "number": 24,
                "name": "\u0627\u0644\u0652\u0645\u064f\u0639\u0650\u0632\u064f\u0651",
                "transliteration": "Al Mu'iz",
                "translation": "The Bestower of Honour",
                "meaning": "He gives esteem to whoever He willed, hence there is no one to degrade Him.",
                "audio": "/audio/asma-ul-husna/muizz.mp3"
            },
            {
                "number": 25,
                "name": "\u0627\u0644\u0645\u064f\u0630\u0650\u0644\u064f\u0651",
                "transliteration": "Al Mudhil",
                "translation": "The Humiliator",
                "meaning": "The One who degrades whoever he willed, hence there is no one to give him esteem.",
                "audio": "/audio/asma-ul-husna/mudhill.mp3"
            },
            {
                "number": 26,
                "name": "\u0627\u0644\u0633\u064e\u0651\u0645\u0650\u064a\u0639\u064f",
                "transliteration": "As Samee'",
                "translation": "The All-Hearing",
                "meaning": "The One who Hears all things that are heard by His Eternal Hearing without an ear, instrument or organ.",
                "audio": "/audio/asma-ul-husna/sami.mp3"
            },
            {
                "number": 27,
                "name": "\u0627\u0644\u0652\u0628\u064e\u0635\u0650\u064a\u0631\u064f",
                "transliteration": "Al Baseer",
                "translation": "The All-Seeing",
                "meaning": "The One who Sees all things that are seen by His Eternal Seeing without a pupil or any other instrument.",
                "audio": "/audio/asma-ul-husna/basir.mp3"
            },
            {
                "number": 28,
                "name": "\u0627\u0644\u0652\u062d\u064e\u0643\u064e\u0645\u064f",
                "transliteration": "Al Hakam",
                "translation": "The Judge",
                "meaning": "The One who is the judge, the arbitrator, the One who has the complete authority to decide between the creatures.",
                "audio": "/audio/asma-ul-husna/hakam.mp3"
            },
            {
                "number": 29,
                "name": "\u0627\u0644\u0652\u0639\u064e\u062f\u0652\u0644\u064f",
                "transliteration": "Al 'Adl",
                "translation": "The Just",
                "meaning": "The One who is entitled to do what He does.",
                "audio": "/audio/asma-ul-husna/adl.mp3"
            },
            {
                "number": 30,
                "name": "\u0627\u0644\u0644\u064e\u0651\u0637\u0650\u064a\u0641\u064f",
                "transliteration": "Al Lateef",
                "translation": "The Subtle One",
                "meaning": "The One who is kind to His slaves and endows upon them.",
                "audio": "/audio/asma-ul-husna/latif.mp3"
            },
            {
                "number": 31,
                "name": "\u0627\u0644\u0652\u062e\u064e\u0628\u0650\u064a\u0631\u064f",
                "transliteration": "Al Khabeer",
                "translation": "The All-Aware",
                "meaning": "The One who knows the truth of things.",
                "audio": "/audio/asma-ul-husna/khabir.mp3"
            },
            {
                "number": 32,
                "name": "\u0627\u0644\u0652\u062d\u064e\u0644\u0650\u064a\u0645\u064f",
                "transliteration": "Al Haleem",
                "translation": "The Forbearing",
                "meaning": "The One who delays the punishment for those who deserve it and then He might forgive them.",
                "audio": "/audio/asma-ul-husna/halim.mp3"
            },
            {
                "number": 33,
                "name": "\u0627\u0644\u0652\u0639\u064e\u0638\u0650\u064a\u0645\u064f",
                "transliteration": "Al 'Azeem",
                "translation": "The Magnificent",
                "meaning": "The One deserving the attributes of Exaltment, Glory, Extolement, and complete Majesty.",
                "audio": "/audio/asma-ul-husna/azim.mp3"
            },
            {
                "number": 34,
                "name": "\u0627\u0644\u0652\u063a\u064e\u0641\u064f\u0648\u0631\u064f",
                "transliteration": "Al Ghafoor",
                "translation": "The Great Forgiver",
                "meaning": "The One who forgives a lot.",
                "audio": "/audio/asma-ul-husna/ghafur.mp3"
            },
            {
                "number": 35,
                "name": "\u0627\u0644\u0634\u064e\u0651\u0643\u064f\u0648\u0631\u064f",
                "transliteration": "Ash Shakoor",
                "translation": "The Rewarder of Thankfulness",
                "meaning": "The One who gives a lot of reward for a little obedience.",
                "audio": "/audio/asma-ul-husna/shakur.mp3"
            },
            {
                "number": 36,
                "name": "\u0627\u0644\u0652\u0639\u064e\u0644\u0650\u064a\u064f\u0651",
                "transliteration": "Al 'Aliyy",
                "translation": "The Most High",
                "meaning": "The One who is clear from the attributes of the creatures.",
                "audio": "/audio/asma-ul-husna/ali.mp3"
            },
            {
                "number": 37,
                "name": "\u0627\u0644\u0652\u0643\u064e\u0628\u0650\u064a\u0631\u064f",
                "transliteration": "Al Kabeer",
                "translation": "The Most Great",
                "meaning": "The One who is greater than everything in status.",
                "audio": "/audio/asma-ul-husna/kabir.mp3"
            },
            {
                "number": 38,
                "name": "\u0627\u0644\u0652\u062d\u064e\u0641\u0650\u064a\u0638\u064f",
                "transliteration": "Al Hafeez",
                "translation": "The Preserver",
                "meaning": "The One who protects whatever and whoever He willed to protect.",
                "audio": "/audio/asma-ul-husna/hafiz.mp3"
            },
            {
                "number": 39,
                "name": "\u0627\u0644\u0645\u064f\u0642\u064a\u0650\u062a",
                "transliteration": "Al Muqeet",
                "translation": "The Nourisher",
                "meaning": "The One who has the Power.",
                "audio": "/audio/asma-ul-husna/muqit.mp3"
            },
            {
                "number": 40,
                "name": "\u0627\u0644\u0652\u062d\u0633\u0650\u064a\u0628\u064f",
                "transliteration": "Al Haseeb",
                "translation": "The Reckoner",
                "meaning": "The One who gives the satisfaction.",
                "audio": "/audio/asma-ul-husna/hasib.mp3"
            },
            {
                "number": 41,
                "name": "\u0627\u0644\u0652\u062c\u064e\u0644\u0650\u064a\u0644\u064f",
                "transliteration": "Al Jaleel",
                "translation": "The Majestic",
                "meaning": "The One who is attributed with greatness of Power and Glory of status.",
                "audio": "/audio/asma-ul-husna/jalil.mp3"
            },
            {
                "number": 42,
                "name": "\u0627\u0644\u0652\u0643\u064e\u0631\u0650\u064a\u0645\u064f",
                "transliteration": "Al Kareem",
                "translation": "The Generous",
                "meaning": "The One who is clear from abjectness.",
                "audio": "/audio/asma-ul-husna/karim.mp3"
            },
            {
                "number": 43,
                "name": "\u0627\u0644\u0631\u064e\u0651\u0642\u0650\u064a\u0628\u064f",
                "transliteration": "Ar Raqeeb",
                "translation": "The Watchful One",
                "meaning": "The One that nothing is absent from Him.",
                "audio": "/audio/asma-ul-husna/raqib.mp3"
            },
            {
                "number": 44,
                "name": "\u0627\u0644\u0652\u0645\u064f\u062c\u0650\u064a\u0628\u064f",
                "transliteration": "Al Mujeeb",
                "translation": "The Responder to Prayer",
                "meaning": "The One who answers the one in need if he asks Him and rescues the yearner if he calls upon Him.",
                "audio": "/audio/asma-ul-husna/mujib.mp3"
            },
            {
                "number": 45,
                "name": "\u0627\u0644\u0652\u0648\u064e\u0627\u0633\u0650\u0639\u064f",
                "transliteration": "Al Waasi'",
                "translation": "The All-Encompassing",
                "meaning": "The Knowledgeable; The One nothing is absent from His knowledge.",
                "audio": "/audio/asma-ul-husna/wasi.mp3"
            },
            {
                "number": 46,
                "name": "\u0627\u0644\u0652\u062d\u064e\u0643\u0650\u064a\u0645\u064f",
                "transliteration": "Al Hakeem",
                "translation": "The Perfectly Wise",
                "meaning": "The One who is correct in His doings.",
                "audio": "/audio/asma-ul-husna/hakim.mp3"
            },
            {
                "number": 47,
                "name": "\u0627\u0644\u0652\u0648\u064e\u062f\u064f\u0648\u062f\u064f",
                "transliteration": "Al Wudood",
                "translation": "The Loving One",
                "meaning": "The One who loves His believing slaves and His believing slaves love Him.",
                "audio": "/audio/asma-ul-husna/wadud.mp3"
            },
            {
                "number": 48,
                "name": "\u0627\u0644\u0652\u0645\u064e\u062c\u0650\u064a\u062f\u064f",
                "transliteration": "Al Majeed",
                "translation": "The Most Glorious One",
                "meaning": "The One who is with perfect Power, High Status, Compassion, Generosity and Kindness.",
                "audio": "/audio/asma-ul-husna/majeed.mp3"
            },
            {
                "number": 49,
                "name": "\u0627\u0644\u0652\u0628\u064e\u0627\u0639\u0650\u062b\u064f",
                "transliteration": "Al Baa'ith",
                "translation": "The Resurrector",
                "meaning": "The One who resurrects His slaves after death for reward and/or punishment.",
                "audio": "/audio/asma-ul-husna/baith.mp3"
            },
            {
                "number": 50,
                "name": "\u0627\u0644\u0634\u064e\u0651\u0647\u0650\u064a\u062f\u064f",
                "transliteration": "Ash Shaheed",
                "translation": "The Witness",
                "meaning": "The Witness, The One from whom nothing is hidden.",
                "audio": "/audio/asma-ul-husna/shahid.mp3"
            },
            {
                "number": 51,
                "name": "\u0627\u0644\u0652\u062d\u064e\u0642\u064f\u0651",
                "transliteration": "Al Haqq",
                "translation": "The Truth",
                "meaning": "The One who truly exists.",
                "audio": "/audio/asma-ul-husna/haqq.mp3"
            },
            {
                "number": 52,
                "name": "\u0627\u0644\u0652\u0648\u064e\u0643\u0650\u064a\u0644\u064f",
                "transliteration": "Al Wakeel",
                "translation": "The Trustee",
                "meaning": "The Trustee, The One who is relied upon for all affairs.",
                "audio": "/audio/asma-ul-husna/wakil.mp3"
            },
            {
                "number": 53,
                "name": "\u0627\u0644\u0652\u0642\u064e\u0648\u0650\u064a\u064f\u0651",
                "transliteration": "Al Qawiyy",
                "translation": "The Possessor of all strength",
                "meaning": "The One with the complete Power.",
                "audio": "/audio/asma-ul-husna/qawi.mp3"
            },
            {
                "number": 54,
                "name": "\u0627\u0644\u0652\u0645\u064e\u062a\u0650\u064a\u0646\u064f",
                "transliteration": "Al Mateen",
                "translation": "The Forceful",
                "meaning": "The Firm, The One with steadfast power who never tires.",
                "audio": "/audio/asma-ul-husna/matin.mp3"
            },
            {
                "number": 55,
                "name": "\u0627\u0644\u0652\u0648\u064e\u0644\u0650\u064a\u064f\u0651",
                "transliteration": "Al Waliyy",
                "translation": "The Protector",
                "meaning": "The Protecting Friend, The One who supports and manages the affairs of His servants.",
                "audio": "/audio/asma-ul-husna/waliy.mp3"
            },
            {
                "number": 56,
                "name": "\u0627\u0644\u0652\u062d\u064e\u0645\u0650\u064a\u062f\u064f",
                "transliteration": "Al Hameed",
                "translation": "The Praiseworthy",
                "meaning": "The praised One who deserves to be praised.",
                "audio": "/audio/asma-ul-husna/hamid.mp3"
            },
            {
                "number": 57,
                "name": "\u0627\u0644\u0652\u0645\u064f\u062d\u0652\u0635\u0650\u064a",
                "transliteration": "Al Muhsi",
                "translation": "The Reckoner",
                "meaning": "The One who the count of things are known to him.",
                "audio": "/audio/asma-ul-husna/muhsi.mp3"
            },
            {
                "number": 58,
                "name": "\u0627\u0644\u0652\u0645\u064f\u0628\u0652\u062f\u0650\u0626\u064f",
                "transliteration": "Al Mubdi",
                "translation": "The Originator",
                "meaning": "The One who started the human being. That is, He created him.",
                "audio": "/audio/asma-ul-husna/mubdi.mp3"
            },
            {
                "number": 59,
                "name": "\u0627\u0644\u0652\u0645\u064f\u0639\u0650\u064a\u062f\u064f",
                "transliteration": "Al Mu'eed",
                "translation": "The Restorer",
                "meaning": "The One who brings back the creatures after death.",
                "audio": "/audio/asma-ul-husna/muid.mp3"
            },
            {
                "number": 60,
                "name": "\u0627\u0644\u0652\u0645\u064f\u062d\u0652\u064a\u0650\u064a",
                "transliteration": "Al Muhiy",
                "translation": "The Giver of Life",
                "meaning": "The One who took out a living human from semen that does not have a soul.",
                "audio": "/audio/asma-ul-husna/muhyi.mp3"
            },
            {
                "number": 61,
                "name": "\u0627\u064e\u0644\u0652\u0645\u064f\u0645\u0650\u064a\u062a\u064f",
                "transliteration": "Al Mumeet",
                "translation": "The Bringer of Death",
                "meaning": "The One who renders the living dead.",
                "audio": "/audio/asma-ul-husna/mumit.mp3"
            },
